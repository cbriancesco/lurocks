@font-face {
    font-family: 'icons';
    src:
        url('../fonts/icons/icons.ttf?uod260') format('truetype'),
        url('../fonts/icons/icons.woff?uod260') format('woff'),
        url('../fonts/icons/icons.svg?uod260#icons') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'icons' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

