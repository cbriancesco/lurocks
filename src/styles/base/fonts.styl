@font-face {
    font-family: 'olivierregular';
    src: url('../fonts/oliver/olivier_demo-webfont.eot');
    src: url('../fonts/oliver/olivier_demo-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/oliver/olivier_demo-webfont.woff2') format('woff2'),
         url('../fonts/oliver/olivier_demo-webfont.woff') format('woff'),
         url('../fonts/oliver/olivier_demo-webfont.ttf') format('truetype'),
         url('../fonts/oliver/olivier_demo-webfont.svg#olivierregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

